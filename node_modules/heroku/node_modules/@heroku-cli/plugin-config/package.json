    "@oclif/command": "^1.4.30",
    "@oclif/config": "^1.6.19",
    "cli-ux": "^4.4.0",
    "edit-string": "^1.1.6",
    "lodash": "^4.17.10",
    "shell-quote": "^1.6.1"
  },
  "devDependencies": {
    "@fancy-test/nock": "^0.1.1",
    "@heroku-cli/schema": "^1.0.3",
    "@oclif/dev-cli": "^1.13.22",
    "@oclif/plugin-help": "^1.2.11",
    "@oclif/test": "^1.0.6",
    "@oclif/tslint": "1.1.2",
    "@types/chai": "^4.1.3",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "5",
    "@types/nock": "^9.1.3",
    "@types/node": "^10.1.2",
    "@types/supports-color": "^5.3.0",
    "chai": "^4.1.2",
    "globby": "8",
    "mocha": "5.2.0",
    "nock": "^9.2.6",
    "nyc": "11.8.0",
    "ts-node": "6.0.5",
    "tslib": "1.9.1",
    "tslint": "5.10.0",
    "typescript": "2.8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/heroku/heroku-cli-plugin-config",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "heroku",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/heroku-cli-plugin-config",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
